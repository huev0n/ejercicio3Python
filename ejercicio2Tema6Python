class Alumno():
    def __init__(self, nombre, nota):
        self.nombre = nombre
        self.nota = nota

    def __str__(self):
        if (self.nota>=5):
            return "El alumno {}, con una nota de {} ha aprobado".format(self.nombre, self.nota)
        else:
            return "El alumno {}, con una nota de {} ha SUSPENDIDO".format(self.nombre, self.nota)

alumno1 = Alumno("Jaimito",4)
print (alumno1)
alumno2 = Alumno("Pepito",8)
print (alumno2)
